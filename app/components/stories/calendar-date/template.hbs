
{{#stories/story-base
    title=title
    subTitle=subTitle
    color="-black"
    width="-w1"
    height="-h1"
    data-ss-colspan="1"
    viewOnly="true"
}}
    <main class="calendar">
        <div class="calendar__day-word">Wednesday</div>
        <div class="dotted-area -all -rounded -dark pad-top-half pad-right-half pad-bottom-half pad-left">
            <span class="calendar__day-number">23</span>
            <span class="calendar__day-suffix">rd</span>
        </div>
        <span class="calendar__month">April</span>
        <span class="calendar__year">2015</span>
    </main>
{{/stories/story-base}}