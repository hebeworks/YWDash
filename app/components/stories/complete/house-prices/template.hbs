            {{#stories/story-slide}}
                {{stories/complete/house-prices/house-price-item houseType="Semi" total=currentMonth.averageSemi change=changeSemi icon="semi"}}
            {{/stories/story-slide}}

            {{#stories/story-slide}}
                {{stories/complete/house-prices/house-price-item houseType="Terraced" total=currentMonth.averageTerraced change=changeTerraced icon="terrace"}}
