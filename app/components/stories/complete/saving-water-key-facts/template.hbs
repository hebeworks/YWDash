
{{#stories/story-base 
    title=title 
    subTitle=subTitle 
	color="-dark-blue" 
    width=width 
    height=height 
    viewOnly=true
    hideTitles=true
    data-ss-colspan="2"}}
    <main class="saving-water-key-facts">
        <article class="saving-water-key-facts__quote">
            {{randomItem}}
        </article>
        {{!--
        {{#stories/story-slider loaded=loaded}}
            {{#each item in items}}
                {{#stories/story-slide}}
                    <article class="saving-water-key-facts__quote">
                        {{item}}
                    </article>
                {{/stories/story-slide}}
            {{/each}}
        {{/stories/story-slider}}
        --}}
    </main>
{{/stories/story-base}}
